	<parameter name="initialDate" class="java.sql.Date"/>
	<parameter name="finalDate" class="java.sql.Date"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
	o.id AS "id",
	o.createdDate AS "createdDate",
	q.description AS "description",
	CASE  o.status
		WHEN 'PENDING' THEN 'Pendente'
		WHEN 'REALIZED' THEN 'Realizada'
		WHEN 'IN_TRANSIT' THEN 'Em Trânsito'
		WHEN 'PARTIALLY_RECEIVED' THEN 'Parc. Recebida'
		WHEN 'RECEIVED' THEN 'Recebida'
		WHEN 'FINALIZED' THEN 'Finalizada'
		WHEN 'CANCELED' THEN 'Cancelada'
		ELSE ''
	END AS "status",
	o.total AS "total"
FROM purchase_orders o 
		INNER JOIN quotation_approvals qa ON qa.id = o.approval
		INNER JOIN quotations q ON q.id = qa.quotation
    WHERE o.supplier = $P{supplier} AND
    		 $X{IN, o.status, statuses} AND 
    		 o.createddate BETWEEN $P{initialDate} AND $P{finalDate}]]>
	</queryString>
