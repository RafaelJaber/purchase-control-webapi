	<queryString language="SQL">
		<![CDATA[SELECT 
	p.name,
	qi.quantity,
	u.abbreviation
	FROM quoted_items qi
		INNER JOIN supplier_item_quotation siq ON siq.quoteditem = qi.id
		INNER JOIN products p ON qi.product = p.id
		INNER JOIN units u ON u.id = qi.unit
			WHERE quotation = $P{quotation} AND
				  siq.supplier = $P{supplier}]]>
	</queryString>
